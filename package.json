{
  "name": "abel-store",
  "version": "1.0.0",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sass:watch": "sass --no-source-map --watch src/styles/global.scss src/styles/global.css",
    "sass:build": "sass --no-source-map src/styles/global.scss src/styles/global.css",
    "develop": "concurrently \"yarn sass:watch\" \"yarn dev\"",
    "prettier:code": "prettier \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:other": "prettier \"**/*.{md,css,scss,yaml,yml,xml,json,html}\"",
    "format:code": "npm run prettier:code -- --write",
    "format:other": "npm run prettier:other -- --write",
    "format": "npm-run-all -p format:*",
    "install_depend": "npm install",
    "remove_depend": "rm -rf node_modules/ yarn.lock package-lock.json .cache dist"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
